<UserControl x:Class="OnlinePlayer.Views.StartPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:c="clr-namespace:OnlinePlayer.Controls"
      xmlns:vm="clr-namespace:OnlinePlayer.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="{StaticResource MainWindowHeight}" d:DesignWidth="{StaticResource MainWindowWidth}">

    <Grid>
        <StackPanel Orientation="Horizontal" Background="Transparent">
            <c:ImageButton BorderThickness="0" Command="{Binding Source={x:Static vm:AssetVideoVM.ExecuteCmd}}" Source="{StaticResource AssetVideo}" Background="Transparent" Width="300" Height="300"/>
            <c:ImageButton BorderThickness="0"  Command="{Binding OnlineVideoCmd}" Source="{StaticResource OnlineVideo}" Background="Transparent" Width="300" Height="300"/>
        </StackPanel>
    </Grid>
</UserControl>
